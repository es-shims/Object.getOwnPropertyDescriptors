{
	"name": "object.getownpropertydescriptors",
	"version": "2.0.3",
	"author": "Jordan Harband",
	"description": "ES2017 spec-compliant shim for `Object.getOwnPropertyDescriptors` that works in ES5.",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"pretest": "npm run --silent lint && es-shim-api --bound",
		"test": "npm run --silent tests-only",
		"posttest": "npm run audit",
		"tests-only": "npm run --silent test:shimmed && npm run --silent test:module",
		"test:shimmed": "node test/shimmed.js",
		"test:module": "node test/index.js",
		"coverage": "covert test/*.js",
		"coverage:quiet": "covert test/*.js --quiet",
		"lint": "eslint .",
		"preaudit": "npm install --package-lock --package-lock-only",
		"audit": "npm audit",
		"postaudit": "rm package-lock.json"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/object.getownpropertydescriptors.git"
	},
	"keywords": [
		"Object.getOwnPropertyDescriptors",
		"descriptor",
		"property descriptor",
		"ES8",
		"ES2017",
		"shim",
		"polyfill",
		"getOwnPropertyDescriptor",
		"es-shim API"
	],
	"dependencies": {
		"define-properties": "^1.1.3",
		"es-abstract": "^1.13.0"
	},
	"devDependencies": {
		"@es-shims/api": "^2.1.2",
		"@ljharb/eslint-config": "^13.1.1",
		"covert": "^1.1.1",
		"eslint": "^5.13.0",
		"replace": "^1.0.1",
		"semver": "^5.6.0",
		"tape": "^4.10.0"
	},
	"testling": {
		"files": [
			"test/index.js",
			"test/shimmed.js"
		],
		"browsers": [
			"iexplore/9.0..latest",
			"firefox/4.0..6.0",
			"firefox/15.0..latest",
			"firefox/nightly",
			"chrome/5.0..10.0",
			"chrome/20.0..latest",
			"chrome/canary",
			"opera/12.0..latest",
			"opera/next",
			"safari/5.0..latest",
			"ipad/6.0..latest",
			"iphone/6.0..latest",
			"android-browser/4.2"
		]
	},
	"engines": {
		"node": ">= 0.8"
	}
}
